  <!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>main</servlet-name>
    <servlet-class>ua.com.testing.controller.MainController</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>test</servlet-name>
    <servlet-class>ua.com.testing.controller.TestController</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>result</servlet-name>
    <servlet-class>ua.com.testing.controller.ResultController</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>main</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>test</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>result</servlet-name>
    <url-pattern>/result</url-pattern>
  </servlet-mapping>


</web-app>
